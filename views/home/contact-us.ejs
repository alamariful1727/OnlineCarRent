<div class="container">
  <section class="my-5" id="contact">

    <!-- Section heading -->
    <h1 class="text-center head-text">Contact us</h1>
    <hr class="head-hr">
    <!-- Section description -->
    <p class="text-center mx-auto pb-5">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Fugit,
      error amet numquam iure provident voluptate esse quasi, veritatis totam voluptas nostrum quisquam eum porro a
      pariatur veniam.</p>

    <!-- Grid row -->
    <div class="row">

      <!-- Grid column -->
      <div class="col-lg-5 mb-lg-0 mb-4">

        <!-- Form with header -->
        <div class="card">
          <div class="card-body">
            <!-- Header -->
            <div class="form-header bg-dark">
              <h3 class="mt-2"><i class="fas fa-envelope"></i> Write to us:</h3>
